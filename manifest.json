
    {
      "name": "Insert Into",
      "version": "1.0.1",
      "manifest_version": 3,
      "description": "Extension will fill (or add to) the current field with a value from a list",
      "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_icon": "icon.png",
        "default_popup": "pop/build/index.html"
      },
      "icons": {
        "128": "icon.png"
      },
      "permissions": [
          "scripting", "storage", "activeTab"
        ]
    }
